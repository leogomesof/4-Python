n=int(input("Digite o número: "))
print(f"O número escolhido foi: {n}\n0x{n}={0*n}")
print(f"1x{n}={1*n}")
print(f"2x{n}={2*n}")
print(f"3x{n}={3*n}")
print(f"4x{n}={4*n}")
print(f"5x{n}={5*n}")
print(f"6x{n}={6*n}")
print(f"7x{n}={7*n}")
print(f"8x{n}={8*n}")
print(f"9x{n}={9*n}")
print(f"10x{n}={10*n}")
